
<odoo>

    <record id="create_care_card_" model="ir.sequence">
        <field name="name">Create Care Card</field>
        <field name="code">card.number</field>
        <field name="active">TRUE</field>
        <field name="prefix">CARD</field>
        <field name="padding">3</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
     </record> 

     <record id="create_card_request_sequence_number" model="ir.sequence">
        <field name="name">Sequence: Request Number</field>
        <field name="code">request.number</field>
        <field name="active">TRUE</field>
        <field name="prefix">REQUEST</field>
        <field name="padding">3</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
     </record> 

     <record id="create_card_request_update_sequence_number" model="ir.sequence">
      <field name="name">Sequence:Update Request Number</field>
      <field name="code">update.request.number</field>
      <field name="active">TRUE</field>
      <field name="prefix">UP REQUEST</field>
      <field name="padding">3</field>
      <field name="number_next">1</field>
      <field name="number_increment">1</field>
   </record> 

</odoo>
