<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="assets_website_linksss" inherit_id="website.assets_frontend">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/css" href="/care_cards/static/css/mdb.min.css"/>
            <link rel="stylesheet" href="/care_cards/static/css/style.css" />
        </xpath>
      </template>
      <template id="portal_input_check" inherit_id="website.assets_frontend" name="Custom Website Assets">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/care_cards/static/js/main.js"></script>
            <script type="text/javascript" src="/care_cards/static/js/mdb.min.js"></script>
        </xpath>
    </template>

   

</odoo>
